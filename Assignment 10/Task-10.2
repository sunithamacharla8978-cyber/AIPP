def find_common_optimized(a, b):
    return list(set(a) & set(b))

print("\nOptimized function tests (unique common elements):")
print(f"find_common_optimized([1, 2, 3], [3, 4, 5]): {find_common_optimized([1, 2, 3], [3, 4, 5])}")
print(f"find_common_optimized([1, 2, 2, 3], [2, 3, 3, 4]): {find_common_optimized([1, 2, 2, 3], [2, 3, 3, 4])}")
print(f"find_common_optimized(['apple', 'banana'], ['banana', 'orange']): {find_common_optimized(['apple', 'banana'], ['banana', 'orange'])}")

