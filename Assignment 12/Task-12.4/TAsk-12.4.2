# Generate x values
x_vals = np.linspace(-3, 3, 400)

# Calculate corresponding y values for f(x)
y_vals = 2*x_vals**3 + 4*x_vals + 5

# Plot the function
plt.figure(figsize=(10, 6))
plt.plot(x_vals, y_vals, label=r'$f(x) = 2x^3 + 4x + 5$', color='blue')
plt.title('Plot of $f(x) = 2x^3 + 4x + 5$')
plt.xlabel('x')
plt.ylabel('f(x)')
plt.grid(True)
plt.axhline(0, color='black',linewidth=0.5)
plt.axvline(0, color='black',linewidth=0.5)
plt.legend()
plt.show()
